{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "babel-node -r dotenv/config ./bin/www",
        "build": "npm run clean && npm run transpile",
        "transpile": "babel ./ --out-dir ../build/server --ignore ./node_modules",
        "clean": "rimraf ../build/server"
    },
    "dependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/node": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "bcrypt": "^5.0.1",
        "connect-mongo": "^4.6.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "express-session": "^1.17.2",
        "express-validator": "^6.13.0",
        "mongoose": "^6.0.13",
        "morgan": "~1.9.1",
        "passport": "^0.5.0",
        "passport-local": "^1.0.0"
    },
    "devDependencies": {
        "dotenv": "^10.0.0",
        "rimraf": "^3.0.2"
    }
}
